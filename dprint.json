{
  "incremental": true,
  "json": {
  },
  "markdown": {
    "lineWidth": 80,
    "textWrap": "always"
  },
  "toml": {
  },
  "includes": ["**/*.{json,md,toml}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json"
